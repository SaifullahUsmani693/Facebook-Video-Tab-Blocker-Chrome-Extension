{
    "manifest_version": 2,    
    "name": "Facebook Video Tab Blocker",
    "version": "0.1.0",
    "content_scripts": [
        {            
            "matches": ["https://www.facebook.com/watch", "https://*.facebook.com/watch*"],
            "js": ["script.js"],
            "css": ["styles.css"]
        }
    ]
}