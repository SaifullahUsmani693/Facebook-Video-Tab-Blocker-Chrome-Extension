{
    "manifest_version": 3,    
    "name": "Facebook Video Tab Blocker",
    "version": "0.1.0",
    "content_scripts": [
        {            
            "matches": ["https://www.facebook.com/*", "https://www.youtube.com/*"],
            "js": ["script.js"],
            "css": ["styles.css"]
        }
    ]
}
